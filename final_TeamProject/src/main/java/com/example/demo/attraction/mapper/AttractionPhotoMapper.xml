<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="attractPhoto">
  <select id="findPCnt" resultType="java.lang.Integer" parameterType="attractPhotoVO">
  	SELECT COUNT(apNo) FROM TBL_ATTRACTPHOTO WHERE ATTRACTNO = #{attractNo}
  </select>
</mapper>


